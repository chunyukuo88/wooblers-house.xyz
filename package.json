{
  "name": "wooblers-house",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest-localstorage-mock": "^2.4.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-redux": "^7.2.4",
    "react-responsive-carousel": "^3.2.18",
    "react-scripts": "4.0.3",
    "reactjs-popup": "^2.0.4",
    "redux": "^4.1.0",
    "redux-promise": "^0.6.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "build": "react-scripts build",
    "coverage": "jest --coverage --colors --silent",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!**/tests/**",
      "!./src/*.js",
      "./src/**/*.js",
      "./src/pages/resetPassword/ResetPassword.jsx",
      "./src/pages/termsOfService/termsOfService.jsx",
      "./src/pages/support/support.jsx",
      "./src/pages/contract/details/contractDetails.jsx",
      "./src/ErrorBoundary.jsx",
      "./src/routeHandler.jsx",
      "./src/pages/page.jsx"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/*.test.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.test.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
